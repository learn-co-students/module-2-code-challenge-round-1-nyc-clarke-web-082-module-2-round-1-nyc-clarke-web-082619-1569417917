<h1> Create A New Heroine </h1>

<%= form_for @heroine do |f| %> 
    <%= f.label :name %> 
    <%= f.text_field :name %> 

    <%= f.label :super_name %> 
    <%= f.text_field :super_name %>

    <%= f.label "Select Power" %> 
    <%= f.collection_select(:power_id, Power.all, :id, :name, prompt: true) %> 

    <%= f.submit %> 

    <% end %> 