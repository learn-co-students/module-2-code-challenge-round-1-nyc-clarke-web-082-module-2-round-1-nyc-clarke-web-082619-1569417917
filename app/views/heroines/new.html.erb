<h1>Heroine Form</h1>

<%= flash[:errors] %>


<%= form_for @heroine do |f| %>
    <label>Name</label>
    <%= f.text_field :name %><br>
    <label>Super Name</label>
    <%= f.text_field :super_name %><br>
    <label>Power</label>
    <%= f.collection_select(:power_id, Power.all, :id, :name)  %><br>
    <%= f.submit %>
<% end %>